{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "my-company-library-library": {
      "components": [
        {
          "entrypoint": "./lib/libraries/myCompanyLibrary/MyCompanyLibraryLibrary.js",
          "manifest": "./src/libraries/myCompanyLibrary/MyCompanyLibraryLibrary.manifest.json"
        }
      ]
    },
    "export-to-csv-library": {
      "components": [
        {
          "entrypoint": "./lib/libraries/exportToCsvLibrary/ExportToCsvLibrary.js",
          "manifest": "./src/libraries/exportToCsvLibrary/ExportToCsvLibrary.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "MyCompanyLibraryLibraryStrings": "lib/libraries/myCompanyLibrary/loc/{locale}.js",
    "ExportToCsvLibraryStrings": "lib/libraries/exportToCsvLibrary/loc/{locale}.js"
  }
}
